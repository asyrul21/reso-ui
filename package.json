{
  "name": "reso-ui",
  "version": "2.0.1",
  "description": "Reusable React components in Typescript",
  "types": "types/index",
  "main": "dist/lib.js",
  "files": [
    "dist",
    "types"
  ],
  "exports": {
    ".": "./dist/lib.js",
    "./styles": "./dist/styles.css"
  },
  "scripts": {
    "setup-commitlint": "husky install && npx husky add .husky/commit-msg \"npx --no-install commitlint --edit \"$1\"\"",
    "disable-commitlint": "rm -f .husky/commit-msg",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "both": "concurrently \"npm run dev\" \"npm run start\"",
    "dev": "npm run storybook",
    "start": "webpack serve --mode development --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js --mode production",
    "lint": "tslint -p . -c tslint.json --exclude '**/*.stories.tsx' --exclude '**/*.test.{ts,tsx}' ",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "refresh-types": "rm -rf types && npm run build",
    "reset-dist": "rm -rf dist && git restore dist"
  },
  "author": "Asyrul Ahmad",
  "license": "ISC",
  "dependencies": {
    "classnames": "^2.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@mdx-js/loader": "^2.2.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-actions": "^8.0.0",
    "@storybook/addon-essentials": "^8.0.0",
    "@storybook/addon-interactions": "^8.0.0",
    "@storybook/addon-links": "^8.0.0",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.3",
    "@storybook/react": "8.0.0-rc.3",
    "@storybook/react-webpack5": "8.0.0-rc.3",
    "@svgr/webpack": "^7.0.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.15",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "babel-loader": "^8.3.0",
    "concurrently": "^7.6.0",
    "css-loader": "^6.7.3",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "mini-css-extract-plugin": "^2.8.0",
    "npm": "^9.2.0",
    "prettier": "^3.2.5",
    "react-router-dom": "^6.22.1",
    "sass": "^1.56.2",
    "sass-loader": "^13.2.0",
    "storybook": "^8.0.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "tslint": "^6.1.3",
    "tslint-react": "^5.0.0",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.9.0"
  }
}
